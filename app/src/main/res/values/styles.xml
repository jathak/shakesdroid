<resources>

    <attr name="icon_tint" format="reference"/>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Material.Light.DarkActionBar">
        <item name="android:textColorSecondary">#ff000000</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:textColorTertiary">#ff000000</item>
        <item name="android:actionBarTabStyle">@style/ABTabs</item>
        <item name="android:colorPrimary">@color/play</item>
        <item name="android:colorPrimaryDark">@color/Dark.play</item>
        <item name="android:colorAccent">@color/play</item>
        <item name="icon_tint">@color/darkBack</item>
        <item name="android:windowBackground">@color/window</item>
        
        <!-- Customize your theme here. -->
        <item name="vpiTabPageIndicatorStyle">@style/TabIndicator</item>
    </style>
    <style name="DarkTheme" parent="android:Theme.Material">
        <item name="android:textColorSecondary">#ff000000</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:textColorTertiary">#ff000000</item>
        <item name="android:actionBarTabStyle">@style/ABTabs</item>
        <item name="android:colorPrimary">@color/play</item>
        <item name="android:colorPrimaryDark">@color/play</item>
        <item name="android:colorAccent">@color/play</item>
        <item name="icon_tint">@color/lightBack</item>
        <item name="android:windowBackground">@color/window</item>
        
        <!-- Customize your theme here. -->
        <item name="vpiTabPageIndicatorStyle">@style/TabIndicator</item>
        <item name="android:textColor">#eee</item>
    </style>
    <style name="TabIndicator" parent="Widget.TabPageIndicator">
        <item name="android:background">@drawable/vpi__tab_indicator</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="android:dividerPadding">3dp</item>
        <item name="android:showDividers">middle</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:paddingRight">12dp</item><!--
        <item name="android:fadingEdge">horizontal</item>
        <item name="android:fadingEdgeLength">8dp</item>-->
    </style>
    <style name="DarkIndicator" parent="TabIndicator">

    </style>
    <style name="ABTabs" parent="@android:style/Widget.ActionBar.TabView">
        <item name="android:background">@drawable/vpi__tab_indicator</item>
    </style>
